    view_xview[0],
    view_yview[0],
    view_wview[0],
    view_hview[0],false,true)
instance_activate_object(obj_invisible_wall);
instance_activate_object(obj_enemy);

///Movement and Collision

if global.endlevel = true exit;

if global.pause = true image_speed = 0
if global.pause = true exit;

//Get the player's input
key_right = keyboard_check(vk_right);
key_left = -keyboard_check(vk_left);
key_jump = keyboard_check_pressed(vk_up);
key_down = keyboard_check(vk_down)

//React to inputs
move = key_left + key_right;
hsp = move * movespeed;
if (vsp &lt; 10) vsp += grav;

if (place_meeting(x,y+1,obj_wall))
{
    if (key_jump) vsp = -jumpspeed
    if !instance_exists(obj_score) global.combo = -1
}

//horizaontal collision
if (place_meeting(x+hsp,y,obj_wall))
{
    while(!place_meeting(x+sign(hsp),y,obj_wall))
    {
        x += sign(hsp);
    }
    hsp = 0;
}
x += hsp;

//vertical collision
if (place_meeting(x,y+vsp,obj_wall))
{
    while(!place_meeting(x,y+sign(vsp),obj_wall))
    {
        y += sign(vsp);
    }
    vsp = 0;
}
y += vsp;


//Direction Facing

if keyboard_check(vk_nokey) = 1
    {
    image_index = 0
    image_speed = 0
    }


if keyboard_check_pressed(vk_right) = 1
    {
    sprite_index = spr_player
    image_index = 0
    image_speed = 0.25
    }

if keyboard_check_pressed(vk_left) = 1
    {
    sprite_index = spr_playerleft
    image_index = 0
    image_speed = 0.25
}

if obj_player.y &gt; 768
{global.pause = true}
</string>
